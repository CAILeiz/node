<!DOCTYPE html>
<html lang="en">
  <head>
    <meta charset="UTF-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <title>Document</title>
  </head>
  <body>
    <!--  action 是端口后面的路径  -->
    <!-- http://localhost:3000/search?searchKey=%E6%94%B6%E5%88%B0as -->
    <form action="/search" method="get">
      <input type="text" name="searchKey" />
      <input type="text" name="searchKey" />
      <input type="submit" value="搜索" />
    </form>

    <script src="https://www.jquery.js/12.js">
      // 上面的表单还可以使用ajax来请求 这里使用query
      let getUrl = "/search?searchKey=故宫"
      $.ajax("get", getUrl).then( res => {
        console.log(res);
      })
    </script>
  </body>
</html>
